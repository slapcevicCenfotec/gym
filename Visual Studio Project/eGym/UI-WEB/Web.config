<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <configSections>
  </configSections>
  <system.web>
    <compilation debug="true" strict="false" explicit="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <defaultDocument enabled="true">
      <files>
        <clear />
        <add value="iniciarsesion.aspx" />
      </files>
    </defaultDocument>
  </system.webServer>
  <connectionStrings>

    <add name="eGymDB" connectionString="Data Source=(local);Initial Catalog=eGymDB;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.serviceModel>
    <behaviors>
      <endpointBehaviors>
        <behavior name="UI_WEB.ServicioEjercicioAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
      multipleSiteBindingsEnabled="true" />
    <services>
      <service name="UI_WEB.ServicioEjercicio">
        <endpoint address="" behaviorConfiguration="UI_WEB.ServicioEjercicioAspNetAjaxBehavior"
          binding="webHttpBinding" contract="UI_WEB.ServicioEjercicio" />
      </service>
    </services>
  </system.serviceModel>
</configuration>
<?xml version="1.0"?>
<configuration>

  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5"/>
  </system.web>
  <system.serviceModel>
    <services>
      <service name="ServicioEnClases.ServicioProyecto">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServicioProyectoAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServicioProyecto" />
      </service>
      <service name="ServicioEnClases.ServicioUsuario">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServicioUsuarioAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServicioUsuario" />
      </service>
      <service name="ServicioEnClases.ServicioTipoPago">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServicioTipoPagoAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServicioTipoPago" />
      </service>
      <service name="ServicioEnClases.ServicioPago">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServicioPagoAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServicioPago" />
      </service>
      <service name="ServicioEnClases.ServicioEjercicio">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServicioEjercicioAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServicioEjercicio" />
      </service>
      <service name="ServicioEnClases.ServiciosTiposDeMaquinas">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServicioTiposDeMaquinasAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServiciosTiposDeMaquinas" />
      </service>
      <service name="ServicioEnClases.ServiciosMaquinas">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServiciosMaquinasAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServiciosMaquinas" />
      </service>
      <service name="ServicioEnClases.ServicioFichasDeMedicion">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServicioFichasDeMedicionAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServicioFichasDeMedicion" />
      </service>
      <service name="ServicioEnClases.ServicioSesion">
        <endpoint address="" behaviorConfiguration="ServicioEnClases.ServicioSesionAspNetAjaxBehavior"
          binding="webHttpBinding" contract="ServicioEnClases.ServicioSesion" />
      </service>
    </services>
    <behaviors>
      <endpointBehaviors>
        <behavior name="ServicioEnClases.ServicioProyectoAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServicioUsuarioAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServicioTipoPagoAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServivioAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServicioPagoAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServicioEjercicioAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServicioTiposDeMaquinasAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServiciosMaquinasAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServicioFichasDeMedicionAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
        <behavior name="ServicioEnClases.ServicioSesionAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
      </endpointBehaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <protocolMapping>
      <add binding="basicHttpsBinding" scheme="https" />
    </protocolMapping>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
      multipleSiteBindingsEnabled="true">
      <serviceActivations>
        <add relativeAddress="ServicioLogin.svc" service="ServicioEnClases.ServicioLogin"
          factory="System.ServiceModel.Activation.WebServiceHostFactory" />
      </serviceActivations>
    </serviceHostingEnvironment>
    
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <!--
        To browse web app root directory during debugging, set the value below to true.
        Set to false before deployment to avoid disclosing web app folder information.
      -->
    <directoryBrowse enabled="true"/>
  </system.webServer>

  <connectionStrings>

    <add name="eGymDB" connectionString="Data Source=cantaros.no-ip.info;Initial Catalog=eGymDB;User ID=Cantaros;Password=cantaros123; Integrated Security=False"
  providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>
